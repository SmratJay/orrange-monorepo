# Orrange Contract Deployment Configuration
# Copy this file to .env and fill in your values

# Private key for deployment (DO NOT commit actual private keys)
PRIVATE_KEY=your_deployment_private_key_here

# RPC URLs for different networks
SEPOLIA_RPC_URL=https://sepolia.infura.io/v3/YOUR_INFURA_PROJECT_ID
GOERLI_RPC_URL=https://goerli.infura.io/v3/YOUR_INFURA_PROJECT_ID
MAINNET_RPC_URL=https://mainnet.infura.io/v3/YOUR_INFURA_PROJECT_ID

# Alternative RPC providers
ALCHEMY_API_KEY=your_alchemy_api_key
QUICKNODE_URL=your_quicknode_url

# Etherscan API keys for verification
ETHERSCAN_API_KEY=your_etherscan_api_key

# Contract Constructor Parameters (optional - will use deployer address if not set)
FEE_COLLECTOR_ADDRESS=0x...
EMERGENCY_MULTISIG_ADDRESS=0x...
INITIAL_OWNER_ADDRESS=0x...

# Gas Configuration (optional)
GAS_PRICE=20000000000
GAS_LIMIT=8000000

# Deployment Settings
CONFIRMATIONS=2

# Test Configuration
TEST_TOKEN_ADDRESS_SEPOLIA=0x...
TEST_USDC_ADDRESS_SEPOLIA=0x...
